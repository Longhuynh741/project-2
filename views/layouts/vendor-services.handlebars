{{!-- BACKGROUND BOX where Event info and Service Cards will be displayed --}}
<div class="grid-container event-box top-space">
 <h2>Welcome <span class="vendor-name"></span></h2>
  {{!-- HEADING --}}
  <h3 class="title-underline">Pending Bids</h3>
  {{!-- SERVICE CARD DISPLAY AREA --}}
  <div class="grid-x grid-margin-x grid-x grid-margin-x small-up-4 medium-up-4 large-up-30">
    {{!-- Service Card --}}
    <div class="cell">
      <div class="card service-card">
        <div class="card-section">
            {{!-- Dynamically Generate the Event Title --}}
             <h5>Event Title</h5>
            {{!-- Dynamically Generate the Event Date  --}}
            <div class=" flex-child-auto"><h6>Date: 12/12/20 {{date}} </h6></div>
            {{!-- Dynamically Generate the Event Time --}}
            <div class=" flex-child-auto"><h6>Time: 1:30-2:30 {{startTime}} - {{endTime}} </h6></div>
            {{!-- Dynamically Generate the Event Location/Address --}}
            <div class=" flex-child-auto"><h6>Location: 275 West Vineland Road, Augusta, GA, 30904</h6></div>
          {{!-- VIEW BIDS leads to Bid List Page --}}
          <a class="button small pink-button" href="#">CANCEL BID</a>
        </div>
      </div>
    </div>
{{!-- Below this line are examples that will need to be deleted until the footer--}}
